this.isLogin = cond\n        },\n        triggerReload(){\n\n            this.getAllPdf()\n\n        },\n\n        showPageToggle(){\n\n        },\n        getAllPdf(){\n\n            let token = localStorage.getItem(\"token\")\n\n            axios({\n                url : \"http://localhost:3000/pdfs\",\n                method : \"GET\",\n                headers : {token}\n            })\n            .then(response=>{\n                console.log(response.data)\n                this.allpdf = response.data\n            })\n            .catch(err=>{\n                console.log(err)\n            })\n        }\n    },\n    created(){\n\n        let token = localStorage.getItem(\"token\")\n\n        if(token){\n            this.isLogin = true\n            this.getAllPdf()\n        }\n        else { \n            this.isLogin = false\n        }\n\n    }\n\n}\n</script>\n\n<style>\n\n</style>\n"},"lineCount":null}},"error":null,"hash":"e3f3cd90b64cffdc3a23e42d92940f60","cacheData":{"env":{}}}