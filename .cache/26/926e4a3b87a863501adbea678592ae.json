  this.isLogin = cond;\n    },\n    triggerReload() {\n      this.getAllPdf();\n    },\n\n    showPageToggle() {},\n    getAllPdf() {\n      let token = localStorage.getItem(\"token\");\n\n      axios({\n        url: \"http://localhost:3000/pdfs\",\n        method: \"GET\",\n        headers: { token }\n      })\n        .then(response => {\n          console.log(response.data);\n          this.allpdf = response.data;\n        })\n        .catch(err => {\n          console.log(err);\n        });\n    }\n  },\n  created() {\n    let token = localStorage.getItem(\"token\");\n\n    if (token) {\n      this.isLogin = true;\n      this.getAllPdf();\n    } else {\n      this.isLogin = false;\n    }\n  }\n};\n</script>\n\n<style>\n</style>\n"},"lineCount":null}},"error":null,"hash":"5968f0d5c80e929aa491ed4aeba6a67e","cacheData":{"env":{}}}